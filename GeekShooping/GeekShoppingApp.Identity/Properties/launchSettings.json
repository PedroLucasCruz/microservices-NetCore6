{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:45808",
      "sslPort": 44368
    }
  },
  "profiles": {
    "IIS - Development": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger", //configurar para pegar o direcionamento para o sweagger quando der o start
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },

    "IIS - Staging": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger", //configurar para pegar o direcionamento para o sweagger quando der o start
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },

    "IIS - Production": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger", //configurar para pegar o direcionamento para o sweagger quando der o start
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },


    "Selfhosting - Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger", //configurar para pegar o direcionamento para o sweagger quando der o start
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },

    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true
      }


    }
}
